require 'sane'
require 'spec/autorun'

describe "analyze" do

  it "should be callable" do
    a = b = 3
    c = analyze a, b, :string => true
    assert c.contain "a="
    assert c.contain "b="

  end

  it "should add spacing"
  it "should get all variables"
  it "should use inspect"
  it "should show linenumber"




end